##transfer name to villager
#air block detection
summon minecraft:area_effect_cloud ~ 255 ~ {Tags:["dan.air_detector"]}
execute as @e[tag=dan.air_detector] at @s run function dan.reap:utility/find_air

#set sign (if villager does or does not have profession)
execute at @e[tag=dan.air_detector] unless entity @e[tag=dan.settingVillagerProfession,sort=nearest,limit=1] run setblock ~ ~-1 ~ minecraft:acacia_sign{Text1:'[{"selector":"@e[tag=dan.settingVillagerName,sort=nearest,limit=1]"},{"text":" "},{"selector":"@e[tag=dan.settingVillagerTitle,sort=nearest,limit=1]"}]'} replace
execute at @e[tag=dan.air_detector] if entity @e[tag=dan.settingVillagerProfession,sort=nearest,limit=1] run setblock ~ ~-1 ~ minecraft:acacia_sign{Text1:'[{"selector":"@e[tag=dan.settingVillagerName,sort=nearest,limit=1]"},{"text":" "},{"selector":"@e[tag=dan.settingVillagerTitle,sort=nearest,limit=1]"},{"text":" "},{"selector":"@e[tag=dan.settingVillagerProfession,sort=nearest,limit=1]"}]'} replace

#edit sign
execute at @e[tag=dan.air_detector] run data modify entity @s CustomName set from block ~ ~-1 ~ Text1

#end air detection/kill sign
execute at @e[tag=dan.air_detector] run setblock ~ ~-1 ~ air
kill @e[tag=dan.air_detector]